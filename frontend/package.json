{
  "name": "entrance-app-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "\n============== MAIN SCRIPTS ==============": "",
    "start": "run-p dev:*",
    "build": "react-scripts build",
    "test": "run-s test:*",
    "\n============== TEST SCRIPTS ==============": "",
    "test:test": "react-scripts test",
    "test:lint": "eslint \"src/**/*.tsx\"",
    "\n============== DEV SCRIPTS ==============": "",
    "dev:frontend": "react-scripts start",
    "dev:backend-proxy": "node src/dev-backend-proxy.js",
    "dev:backend-server": "cd .. && docker-compose up",
    "\n============== MISC SCRIPTS ==============": "",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@material-ui/core": "4.10.0",
    "dayjs": "1.8.28",
    "history": "4.10.1",
    "lodash": "4.17.15",
    "qhistory": "1.0.3",
    "query-string": "6.12.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-query": "1.5.9",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "@types/jest": "24.0.0",
    "@types/node": "12.0.0",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "5.1.5",
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "@typescript-eslint/parser": "3.1.0",
    "http-proxy": "1.18.1",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "react-scripts": "3.4.1",
    "typescript": "3.7.2"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-empty-interface": "off",
      "@typescript-eslint/no-explicit-any": "off"
    }
  },
  "prettier": {
    "printWidth": 100
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
